<br>
<br>
<div class="container-fluid">
  <div class=row>
    <div class="col col-md-12">
      <a href="account/create" class="btn btn-primary new">
        Create a new account
      </a>

      <h2>Recent entries</h2>
      <p>There are
        <%= account.query.length %> accounts.
        <br>
      </p>
      <% for (var i = account.query.length-1; i >= 0; i--) {   
        account = account.query[i];   %>
      <div class="panel panel-default">
        <div class="panel-heading"></div>
        <div class="text-muted pull-right"></div>
        <div class="panel-body">

          <br>
          <br> ID: <%=  account._id %>
          <br> acountNum: <%=account.acountNum%>
          <br> balance: <%= account.balance %>
          <br> routingNum: <%= account.routingNum %>
          <br>
          <hr>
          <a href="/account/delete/<%= account._id  %>" class="btn btn-danger"> Delete </a>
          <a href="/account/details/<%= account._id %>" class="btn btn-info"> Details </a>
          <a href="/account/edit/<%= account._id %>" class="btn btn-primary"> Edit </a>
          <hr>
          <hr>
        </div>
      </div>
      <% } %>

    </div>
  </div>
</div>